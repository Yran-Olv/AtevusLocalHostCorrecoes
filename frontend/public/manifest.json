{
  "short_name": "Multivus",
  "name": "Multivus - Sistema de Atendimento WhatsApp",
  "description": "Sistema de atendimento pelo WhatsApp. Permite que várias pessoas usem o mesmo número de WhatsApp ao mesmo tempo, para responder clientes de forma organizada e profissional.",
  "icons": [
    {
      "src": "favicon.ico",
      "sizes": "64x64 32x32 24x24 16x16",
      "type": "image/x-icon"
    },
    {
      "src": "/android-chrome-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/android-chrome-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "id": "com.multivus.app",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "orientation": "portrait-primary",
  "theme_color": "#128c7e",
  "background_color": "#128c7e",
  "categories": ["business", "productivity", "communication"],
  "prefer_related_applications": false,
  "screenshots": [],
  "shortcuts": [
    {
      "name": "Tickets",
      "short_name": "Tickets",
      "description": "Ver tickets de atendimento",
      "url": "/tickets",
      "icons": [
        {
          "src": "/android-chrome-192x192.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+multivus",
      "url": "/?action=%s"
    }
  ],
  "related_applications": [],
  "iarc_rating_id": "",
  "dir": "ltr",
  "lang": "pt-BR",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  }
}
